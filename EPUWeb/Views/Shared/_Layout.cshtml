<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>ĐẠI HỌC ĐIỆN LỰC- EPU</title>
    <!-- Favicone Icon -->
    <!--<link rel="shortcut icon" type="image/x-icon" href="http://theembazaar.com/tf/educate/favicon.ico">-->
    <link href="https://fonts.googleapis.com/css?family=Raleway:300,400,600,700,800%7CLato:300,400,700" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/Content/css/bootstrap.css" rel="stylesheet" type="text/css">
    <link href="~/Content/css/font-awesome.css" rel="stylesheet" type="text/css">

    <link href="~/Content/css/ionicons.css" rel="stylesheet" type="text/css">
    <!--Light box-->
    <link href="~/Content/css/jquery.fancybox.css" rel="stylesheet" type="text/css">
    <!-- carousel -->
    <!-- PrettyPhoto Popup -->
    <link href="~/Content/css/prettyPhoto.css" rel="stylesheet" />
    <link href="~/Content/css/owl.carousel.css" rel="stylesheet" type="text/css">
    <!--Main Slider-->
    <link href="~/Content/css/settings.css" type="text/css" rel="stylesheet" media="screen">
    <link href="~/Content/css/layers.css" type="text/css" rel="stylesheet" media="screen">
    <link href="~/Content/css/style.css" rel="stylesheet">
    <link href="~/Content/css/bootsnav.css" rel="stylesheet">
    <link href="~/Content/css/index1.css" rel="stylesheet">

</head>
<body>
    <!--loader-->
    <div id="preloader">
        <div class="sk-circle">
            <div class="sk-circle1 sk-child"></div>
            <div class="sk-circle2 sk-child"></div>
            <div class="sk-circle3 sk-child"></div>
            <div class="sk-circle4 sk-child"></div>
            <div class="sk-circle5 sk-child"></div>
            <div class="sk-circle6 sk-child"></div>
            <div class="sk-circle7 sk-child"></div>
            <div class="sk-circle8 sk-child"></div>
            <div class="sk-circle9 sk-child"></div>
            <div class="sk-circle10 sk-child"></div>
            <div class="sk-circle11 sk-child"></div>
            <div class="sk-circle12 sk-child"></div>
        </div>
    </div>
    <!--loader-->
    <!--Header Section Start Here
    ==================================-->
    @Html.Partial("_Header")    
    <!--Header End Here-->
    <!--  Main Banner Start Here-->
    @Html.Partial("_Slide")
    <!--  Main Banner End Here-->
    <!--top Section start Here-->
    @RenderBody()
    <!--top Section End Here-->
    <!--Footer Section Start-->
    @Html.Partial("_Footer")
    <!--Footer Section End-->
    <!-- Site Wraper End -->
    <script src="~/Scripts/js/jquery.min.js" type="text/javascript"></script>
    <!-- Easing Effect Js -->
    <script src="~/Scripts/js/jquery.easing.js" type="text/javascript"></script>
    <!-- bootstrap Js -->
    <script src="~/Scripts/js/bootstrap.min.js" type="text/javascript"></script>
    <!-- fancybox Js -->
    <script src="~/Scripts/js/jquery.mousewheel-3.0.6.pack.js" type="text/javascript"></script>
    <script src="~/Scripts/js/jquery.fancybox.pack.js" type="text/javascript"></script>
    <!-- carousel Js -->
    <script src="~/Scripts/js/owl.carousel.js" type="text/javascript"></script>
    <!-- imagesloaded Js -->
    <script src="~/Scripts/js/imagesloaded.pkgd.min.js" type="text/javascript"></script>
    <!-- masonry,isotope Effect Js -->
    <script src="~/Scripts/js/imagesloaded.pkgd.min.js" type="text/javascript"></script>
    <script src="~/Scripts/js/isotope.pkgd.min.js" type="text/javascript"></script>
    <script src="~/Scripts/js/masonry.pkgd.min.js" type="text/javascript"></script>
    <script src="~/Scripts/js/jquery.appear.js" type="text/javascript"></script>
    <!-- Height Js -->
    <script src="~/Scripts/js/jquery.matchHeight-min.js" type="text/javascript"></script>
    <!-- parallax Js -->
    <script src="~/Scripts/js/jquery.parallax-1.1.3.js" type="text/javascript"></script>
    <!-- revolution Js -->
    <script type="text/javascript" src="~/Scripts/js/jquery.themepunch.tools.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/jquery.themepunch.revolution.min.js"></script>
    <script type="text/javascript" src="~/Scripts/extensions/revolution.extension.slideanims.min.js"></script>
    <script type="text/javascript" src="~/Scripts/extensions/revolution.extension.layeranimation.min.js"></script>
    <script type="text/javascript" src="~/Scripts/extensions/revolution.extension.navigation.min.js"></script>
    <script type="text/javascript" src="~/Scripts/extensions/revolution.extension.parallax.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/jquery.revolution.js"></script>
    <!-- custom Js -->
    <script src="~/Scripts/js/custom.js" type="text/javascript"></script>
</body>
</html>


